{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "symfony/console": "6.0.*",
    "symfony/dotenv": "6.0.*",
    "symfony/flex": "^1.17|^2",
    "symfony/framework-bundle": "6.0.*",
    "symfony/runtime": "6.0.*",
    "symfony/security-bundle": "6.0.*",
    "symfony/yaml": "6.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.6",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "3.*"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "CountryCheckApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CountryCheckApi\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "tests": "phpunit"
  }
}
